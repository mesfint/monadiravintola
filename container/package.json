{
  "name": "container",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "build:sw": "webpack --config config/webpack.sw.config.js",
    "build:app": "webpack --config config/webpack.config.js",
    "build": "pnpm build:app && pnpm build:sw",
    "dev:sw": "webpack --config config/webpack.sw.config.js --watch",
    "start": "concurrently \"webpack serve --config config/webpack.config.js\" \"pnpm dev:sw\"",
    "demo": "DEMO_MODE=true webpack serve",
    "analyze": "cross-env NODE_ENV=production webpack --config config/webpack.config.js --analyze",
    "clean": "rm -rf dist",
    "dev": "vite",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.25.9",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.3.1",
    "@mui/material": "^6.3.1",
    "css-loader": "^7.1.2",
    "dotenv": "^16.4.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^5.0.0",
    "react-router-dom": "^6.30.0",
    "style-loader": "^4.0.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-merge": "^6.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@eslint/js": "^9.18.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "babel-loader": "^9.2.1",
    "concurrently": "^9.1.2",
    "copy-webpack-plugin": "^12.0.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "html-webpack-plugin": "^5.6.3",
    "ts-loader": "^9.5.2",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  }
}
